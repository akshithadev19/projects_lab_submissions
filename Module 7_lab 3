    {
      "cell_type": "code",
      "source": [
        "# Import necessary libraries\n",
        "import numpy as np\n",
        "from sklearn.feature_extraction.text import CountVectorizer, TfidfVectorizer\n",
        "from sklearn.feature_extraction.text import HashingVectorizer\n",
        "\n",
        "# Sample documents\n",
        "documents = [\n",
        "    \"I love programming.\",\n",
        "    \"I love machine learning.\",\n",
        "    \"I enjoy learning new things in programming.\"\n",
        "]\n",
        "\n",
        "# 1. Simple Bag of Words (BoW)\n",
        "print(\"---- Simple Bag of Words ----\")\n",
        "vectorizer = CountVectorizer()\n",
        "X_bow = vectorizer.fit_transform(documents)\n",
        "print(\"Vocabulary:\", vectorizer.get_feature_names_out())\n",
        "print(\"BoW Matrix:\\n\", X_bow.toarray())\n",
        "print()\n",
        "\n",
        "# 2. Term Frequency (TF)\n",
        "print(\"---- Term Frequency (TF) ----\")\n",
        "# In BoW, TF is computed automatically by dividing term counts by the document length.\n",
        "X_tf = vectorizer.transform(documents)\n",
        "# Convert to array for element-wise division\n",
        "X_tf_array = X_tf.toarray()\n",
        "print(\"TF Matrix:\\n\", X_tf_array / X_tf_array.sum(axis=1)[:, np.newaxis])  # Normalize by row (document length)\n",
        "print()\n",
        "\n",
        "# 3. TF-IDF (Term Frequency - Inverse Document Frequency)\n",
        "print(\"---- TF-IDF ----\")\n",
        "tfidf_vectorizer = TfidfVectorizer()\n",
        "X_tfidf = tfidf_vectorizer.fit_transform(documents)\n",
        "print(\"TF-IDF Matrix:\\n\", X_tfidf.toarray())\n",
        "print()\n",
        "\n",
        "# 4. N-Grams (Bigram in this case)\n",
        "print(\"---- N-Grams (Bigram) ----\")\n",
        "bigram_vectorizer = CountVectorizer(ngram_range=(2, 2))  # Bigram\n",
        "X_bigrams = bigram_vectorizer.fit_transform(documents)\n",
        "print(\"Bigram Vocabulary:\", bigram_vectorizer.get_feature_names_out())\n",
        "print(\"Bigram Matrix:\\n\", X_bigrams.toarray())\n",
        "print()\n",
        "\n",
        "# 5. Binary Bag of Words\n",
        "print(\"---- Binary Bag of Words ----\")\n",
        "binary_vectorizer = CountVectorizer(binary=True)\n",
        "X_binary = binary_vectorizer.fit_transform(documents)\n",
        "print(\"Binary BoW Matrix:\\n\", X_binary.toarray())\n",
        "print()\n",
        "\n",
        "# 6. Hashing Trick (for large vocabularies)\n",
        "print(\"---- Hashing Trick ----\")\n",
        "hashing_vectorizer = HashingVectorizer(n_features=10)  # Reduce dimensions to 10 features\n",
        "X_hashing = hashing_vectorizer.transform(documents)\n",
        "print(\"Hashing Matrix (Reduced Dimensions):\\n\", X_hashing.toarray())\n"
      ],
      "metadata": {
        "id": "2_0o7cAImSjr",
        "outputId": "c7f40897-0f2d-4035-808a-8df08b38ac56",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
