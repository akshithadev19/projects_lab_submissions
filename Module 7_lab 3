    {
      "cell_type": "code",
      "source": [
        "# Import necessary libraries for plotting\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from sklearn.feature_extraction.text import CountVectorizer, TfidfVectorizer\n",
        "\n",
        "# Sample documents\n",
        "documents = [\n",
        "    \"I love programming.\",\n",
        "    \"I love machine learning.\",\n",
        "    \"I enjoy learning new things in programming.\"\n",
        "]\n",
        "\n",
        "# Create the Bag of Words (BoW) model\n",
        "vectorizer = CountVectorizer()\n",
        "X_bow = vectorizer.fit_transform(documents)\n",
        "\n",
        "# Convert BoW to array and get the feature names\n",
        "X_bow_array = X_bow.toarray()\n",
        "vocabulary = vectorizer.get_feature_names_out()\n",
        "\n",
        "# Create the Term Frequency (TF) model\n",
        "X_tf = vectorizer.transform(documents)\n",
        "X_tf_array = X_tf.toarray()\n",
        "X_tf_normalized = X_tf_array / X_tf_array.sum(axis=1)[:, np.newaxis]  # Normalize by row\n",
        "\n",
        "# Create the TF-IDF model\n",
        "tfidf_vectorizer = TfidfVectorizer()\n",
        "X_tfidf = tfidf_vectorizer.fit_transform(documents)\n",
        "X_tfidf_array = X_tfidf.toarray()\n",
        "\n",
        "# Plotting the Term Frequency, BoW and TF-IDF for each document\n",
        "\n",
        "# Set up subplots for different visualizations\n",
        "fig, axs = plt.subplots(1, 3, figsize=(18, 6))\n",
        "\n",
        "# Plot BoW values (term frequencies)\n",
        "axs[0].bar(vocabulary, X_bow_array.sum(axis=0), color='skyblue')\n",
        "axs[0].set_title('Term Frequency (BoW)')\n",
        "axs[0].set_xlabel('Terms')\n",
        "axs[0].set_ylabel('Count')\n",
        "axs[0].tick_params(axis='x', rotation=45)\n",
        "\n",
        "# Plot normalized Term Frequency (TF)\n",
        "axs[1].bar(vocabulary, X_tf_normalized.sum(axis=0), color='lightgreen')\n",
        "axs[1].set_title('Normalized Term Frequency (TF)')\n",
        "axs[1].set_xlabel('Terms')\n",
        "axs[1].set_ylabel('Normalized Count')\n",
        "axs[1].tick_params(axis='x', rotation=45)\n",
        "\n",
        "# Plot TF-IDF scores\n",
        "axs[2].bar(vocabulary, X_tfidf_array.sum(axis=0), color='salmon')\n",
        "axs[2].set_title('TF-IDF Scores')\n",
        "axs[2].set_xlabel('Terms')\n",
        "axs[2].set_ylabel('TF-IDF Score')\n",
        "axs[2].tick_params(axis='x', rotation=45)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# Check the scores (for example, sum of values for each document representation)\n",
        "print(\"\\nSum of TF Scores for each document:\")\n",
        "print(X_tf_normalized.sum(axis=1))\n",
        "\n",
        "print(\"\\nSum of TF-IDF Scores for each document:\")\n",
        "print(X_tfidf_array.sum(axis=1))\n"
      ],
