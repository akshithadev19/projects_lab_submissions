        "3. https://towardsdatascience.com/recommendation-system-matrix-factorization-d61978660b4b\n",
        "4. https://analyticsindiamag.com/singular-value-decomposition-svd-application-recommender-system/\n",
        "5. https://machinelearningmastery.com/gentle-introduction-bag-words-model/"
      ]
